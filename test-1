local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UIListLayout = Instance.new("UIListLayout")

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BackgroundTransparency = 1.000
Frame.Position = UDim2.new(0.010416667, 0, 0.0227531288, 0)
Frame.Size = UDim2.new(0, 1880, 0, 24)

UIListLayout.Parent = Frame
UIListLayout.FillDirection = Enum.FillDirection.Horizontal
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder


local Library = {} 

function Library:CreateFrame(name)
	local Frame_2 = Instance.new("Frame")
	local UIListLayout_2 = Instance.new("UIListLayout")
	local Frame_3 = Instance.new("Frame")
	local Main = Instance.new("Frame")
	local Up = Instance.new("Frame")
	local CutFrame = Instance.new("Frame")
	local Ttitle = Instance.new("TextLabel")
	local Hide = Instance.new("TextButton")
	local AAAAAAAAAAAAAA = Instance.new("Frame")
	local UIListLayout_3 = Instance.new("UIListLayout")
	
	Frame_2.Parent = Frame
	Frame_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Frame_2.BackgroundTransparency = 1.000
	Frame_2.BorderSizePixel = 0
	Frame_2.Position = UDim2.new(0.0114583336, 0, 0.01592719, 0)
	Frame_2.Size = UDim2.new(0, 220, 0, 108)
	
	UIListLayout_2.Parent = Frame_2
	UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout_2.VerticalAlignment = Enum.VerticalAlignment.Center
	
	Frame_3.Parent = Frame_2
	Frame_3.Name = name
	Frame_3.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
	Frame_3.BorderSizePixel = 0
	Frame_3.ClipsDescendants = true
	Frame_3.Position = UDim2.new(0.432812512, 0, 0.39231962, 0)
	Frame_3.Size = UDim2.new(0, 194, 0, 109)
	
	Main.Name = "Main"
	Main.Parent = Frame_3
	Main.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
	Main.BackgroundTransparency = 1.000
	Main.Size = UDim2.new(0, 194, 0, 108)
	
	Up.Name = "Up"
	Up.Parent = Frame_3
	Up.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Up.BackgroundTransparency = 1.000
	Up.Size = UDim2.new(0, 194, 0, 25)
	
	CutFrame.Name = "CutFrame"
	CutFrame.Parent = Up
	CutFrame.BackgroundColor3 = Color3.fromRGB(255, 102, 0)
	CutFrame.BorderSizePixel = 0
	CutFrame.Position = UDim2.new(0, 0, 0.96757412, 0)
	CutFrame.Size = UDim2.new(0, 194, 0, 1)
	
	Ttitle.Name = "Ttitle"
	Ttitle.Parent = Up
	Ttitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Ttitle.BackgroundTransparency = 1.000
	Ttitle.Position = UDim2.new(0, 0, -0.0399999619, 0)
	Ttitle.Size = UDim2.new(0, 194, 0, 25)
	Ttitle.Font = Enum.Font.SourceSansBold
	Ttitle.Text = name
	Ttitle.TextColor3 = Color3.fromRGB(255, 255, 255)
	Ttitle.TextSize = 24.000
	
	Hide.Name = "Hide"
	Hide.Parent = Frame_3
	Hide.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Hide.BackgroundTransparency = 1.000
	Hide.Position = UDim2.new(0.871134043, 0, -0.00152168889, 0)
	Hide.Size = UDim2.new(0, 25, 0, 25)
	Hide.Font = Enum.Font.SourceSansBold
	Hide.Text = "-"
	Hide.TextColor3 = Color3.fromRGB(255, 255, 255)
	Hide.TextSize = 23.000
	
	AAAAAAAAAAAAAA.Name = "AAAAAAAAAAAAAA"
	AAAAAAAAAAAAAA.Parent = Main
	AAAAAAAAAAAAAA.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	AAAAAAAAAAAAAA.BackgroundTransparency = 1.000
	AAAAAAAAAAAAAA.Size = UDim2.new(0, 194, 0, 32)
	
	UIListLayout_3.Parent = Main
	UIListLayout_3.HorizontalAlignment = Enum.HorizontalAlignment.Center
	
	Hide.MouseButton1Click:connect(function()
		if Hide.Text == "+" then
			Hide.Text = "-"
			Frame_3:TweenSize(UDim2.new(0, 194, 0, 108), 'In', 'Sine', 0.3, true)
		else
			Hide.Text = "+"
			Frame_3:TweenSize(UDim2.new(0, 194, 0, 25), 'In', 'Sine', 0.3, true)
		end
	end)
	
	local UserInputService = game:GetService("UserInputService")

	local gui = Frame_2
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
	dragging = true
	dragStart = input.Position
	startPos = gui.Position
	
	input.Changed:Connect(function()
	if input.UserInputState == Enum.UserInputState.End then
	dragging = false
	end
	end)
	end
	end)
	
	gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	dragInput = input
	end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
	update(input)
	end
	end)
	
	wait()
	UIListLayout_3:Destroy()
	
	local TLHLibrary = {}
	
	function TLHLibrary:NewButton(name, callback)
		local callback = callback or function() end
		
		local TextButtonFrame = Instance.new("Frame")
		local TextButton = Instance.new("TextButton")
		local UIListLayout_4 = Instance.new("UIListLayout")
		
		TextButtonFrame.Name = "TextButtonFrame"
		TextButtonFrame.Parent = Main
		TextButtonFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextButtonFrame.BackgroundTransparency = 1.000
		TextButtonFrame.Size = UDim2.new(0, 194, 0, 30)
		
		TextButton.Parent = TextButtonFrame
		TextButton.Name = name
		TextButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
		TextButton.BorderSizePixel = 0
		TextButton.Size = UDim2.new(0, 182, 0, 20)
		TextButton.Font = Enum.Font.SourceSansBold
		TextButton.Text = name
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextButton.TextSize = 18.000
		
		UIListLayout_4.Parent = TextButtonFrame
		UIListLayout_4.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout_4.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout_4.VerticalAlignment = Enum.VerticalAlignment.Center
		
		TextButton.MouseButton1Click:connect(function()
			pcall(callback)
		end)
	end
	
	function TLHLibrary:NewLabel(name)
		local TextLabel = Instance.new("TextLabel")
		
		TextLabel.Parent = Main
		TextLabel.Active = true
		TextLabel.Name = name
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.Position = UDim2.new(0.0412371121, 0, 0.298913032, 0)
		TextLabel.Selectable = true
		TextLabel.Size = UDim2.new(0, 194, 0, 20)
		TextLabel.Font = Enum.Font.SourceSansBold
		TextLabel.Text = name
		TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.TextSize = 18.000
	end
	
	function TLHLibrary:NewTextBox(name)
		local TextBox = Instance.new("TextBox")
		
		TextBox.Parent = Main
		TextBox.Name = name
		TextBox.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
		TextBox.BorderColor3 = Color3.fromRGB(48, 48, 48)
		TextBox.BorderSizePixel = 0
		TextBox.Position = UDim2.new(0, 0, 0.0869565234, 0)
		TextBox.Size = UDim2.new(0, 194, 0, 20)
		TextBox.Font = Enum.Font.SourceSansBold
		TextBox.PlaceholderColor3 = Color3.fromRGB(188, 188, 188)
		TextBox.PlaceholderText = name
		TextBox.Text = ""
		TextBox.TextColor3 = Color3.fromRGB(188, 188, 188)
		TextBox.TextSize = 18.000
	end
	
	return TLHLibrary;
end
return Library;
